                      <div class="fw-bold display-6">101</div>
                      <div class="text-inverse text-opacity-50">Simple</div>
                    </div>
                    <div class="pos-checkout-table-info small">
                      <div class="row">
                        <div class="col-6 d-flex justify-content-center">
                          <div class="w-20px"><i class="bi bi-currency-euro text-inverse text-opacity-50"></i></div>
                          <div class="w-60px">35 €</div>
                        </div>
                        <div class="col-6 d-flex justify-content-center">
                          <div class="w-20px"><i class="bi bi-person text-inverse text-opacity-50"></i></div>
                          <div class="w-60px">1 pers.</div>
                        </div>
                      </div>
                      <div class="row">
                        <div class="col-12 d-flex justify-content-center">
                          <a href="hotel_reservation_new.html" class="btn btn-xs btn-primary mt-2">Réserver</a>
