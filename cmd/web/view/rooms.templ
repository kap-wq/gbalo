      <div class="pos-container card-body">
        <div class="pos-header d-flex align-items-center">
          <!-- Back button -->
          <div class="me-2">
            <a href="javascript:history.back()" class="btn btn-default btn-sm">
              <i class="fa fa-arrow-left me-1"></i> Retour
            </a>
          </div>

          <div class="logo">
            <a href="hotel_rooms.html">
              <div class="logo-img"><i class="bi bi-house-door" style="font-size: 1.5rem;"></i></div>
              <div class="logo-text">Hôtel Piscill</div>
            </a>
          </div>

          <div class="ms-auto">
            <a href="/hotel/reservation" class="btn btn-outline-theme"><i class="fa fa-plus-circle me-1"></i> Nouvelle réservation</a>
          </div>
        </div>
        <div class="pos-content">
          <div class="pos-content-container p-3" data-scrollbar="true" data-height="100%">
            <div class="row gx-3">
              <div class="col-xl-3 col-lg-4 col-md-6 pb-3">
                <div class="pos-checkout-table available card">
                  <a href="#" class="pos-checkout-table-container">
                    <div class="pos-checkout-table-header">
                      <div class="status"><i class="bi bi-circle-fill text-success"></i></div>
